  "engines": {
    "npm": "use pnpm instead of npm",
    "yarn": "use pnpm instead of yarn",
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@8.15.5",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack serve --open --config webpack.dev.js",
