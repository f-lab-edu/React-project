    "preinstall": "npx only-allow pnpm",
    "husky": "husky install",
    "prepare": "husky && husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "prettier --list-different"
    ]
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.14.1",
